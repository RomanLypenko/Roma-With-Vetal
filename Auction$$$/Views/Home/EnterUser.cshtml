@model Auction___.Models.UsersModel
@{
    ViewData["Title"] = "View";
}

<form asp-action="EnterPass" asp-controller="home">
    <div class="form-group">
        <label asp-for="UserName" class="control-label">UserName</label>
        <input type="text" asp-for="UserName" value="" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UserPassword" class="control-label">Password</label>
        <input type="text" asp-for="UserPassword" value="" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>

