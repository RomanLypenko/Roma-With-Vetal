@model Auction___.Models.UsersModel
@{
    ViewData["Title"] = "MyLots";
    Layout = "~/Views/_Layout2.cshtml";
}

<h1>MyLots</h1>
<table>
    <tr>
        <td>Name</td>
        <td>UsreName</td>
        <td>InitialPrice</td>

    </tr>

    @{
        foreach (var item in Model.MyLots)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item?.User?.UserName</td>
                <td>@item.InitialPrice</td>
                <td>
                    <a asp-action="Details" asp-controller="Home" asp-route-id="@item.Id" asp-route-name="@item.Name">details</a>
                    <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Edit</a>
                    <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>

        }

    }


</table>


